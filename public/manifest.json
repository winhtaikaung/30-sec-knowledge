{
  "name": "Chrome Extension Boilerplate",
  "description": "Chrome Extension boilerplate created with React Typescript.",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "default_popup": "/index.html#/setting"
  },
  "background": {
    "persistent": false,
    "page": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
